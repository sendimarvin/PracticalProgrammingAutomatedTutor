<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bt_in_class_autograder.AppController">
   <children>
      <VBox prefHeight="400.0" prefWidth="400.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Question" />
            <TextArea fx:id="objQn" prefHeight="200.0" prefWidth="200.0" />
            <Label text="Objective A:" />
            <TextField fx:id="objA" />
            <Label text="Objective B:" />
            <TextField fx:id="objB" />
            <Label text="Objective C:" />
            <TextField fx:id="objC" />
            <Label text="Objective D:" />
            <TextField fx:id="objD" />
            <Label text="Objective E:" />
            <TextField fx:id="objE" />
            <Label text="Objective F:" />
            <TextField fx:id="objF">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TextField>
            <Label text="Answer:" />
            <TextField fx:id="objAns">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <HBox>
               <children>
                  <Button fx:id="addObjQn" mnemonicParsing="false" onAction="#onSubmitObj" prefWidth="200.0" text="Submit" />
                  <Button fx:id="cancelObj" mnemonicParsing="false" onAction="#onCancelObj" prefWidth="200.0" text="Cancel" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
